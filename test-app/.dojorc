{
	"build-app": {
		"locale": "en",
		"supportedLocales": [ "es" ],
		"features": {
			"foo": true,
			"bar": false
		},
		"bundles": {
			"widgets": [
				"src/Bar",
				"src/Baz"
			],
			"lazy": [
				"src/LazyWidget"
			]
		},
		"pwa": {
			"manifest": {
				"name": "Test App",
				"description": "A test application for @dojo/cli-build-app"
			}
		},
		"build-time-render": {
			"root": "app"
		},
		"externals": {
			"dependencies": [
				{
					"name": "third-party",
					"from": "external/third-party",
					"to": "third-party/v2.0.0/",
					"inject": "third-party.js"
				}
			]
		}
	}
}
